{
  "name": "artblocksbot",
  "version": "1.0.0",
  "description": "A bot to provide info for artblocks",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "codegen": "yarn codegen-hasura && yarn codegen-subgraph",
    "codegen-hasura": "graphql-codegen --require dotenv/config --config ./graphql.hasura.config.js dotenv_config_path=./.env",
    "codegen-subgraph": "graphql-codegen --require dotenv/config --config ./graphql.subgraph.config.js dotenv_config_path=./.env"
  },
  "keywords": [
    "artblocks",
    "discord",
    "crypto",
    "eth"
  ],
  "author": "duggan roberts",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/utils": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "axios": "^1.1.3",
    "body-parser": "^1.15.2",
    "discord.js": "^14.6.0",
    "dotenv": "^8.2.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.7.0",
    "googleapis": "^92.0.0",
    "graphql": "^16.6.0",
    "jest": "^28.0.3",
    "lodash.deburr": "^4.1.0",
    "ms": "^2.0.0",
    "node-fetch": "^2.6.1",
    "node-html-parser": "^3.2.0",
    "prettier": "^2.6.2",
    "reconnecting-websocket": "^4.4.0",
    "timeago.js": "^4.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "urql": "^3.0.3",
    "web3": "^1.7.0",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@ardatan/aggregate-error": "^0.0.6",
    "@graphql-codegen/cli": "^2.13.8",
    "@graphql-codegen/typed-document-node": "^2.3.5",
    "@graphql-codegen/typescript": "^2.8.0",
    "@graphql-codegen/typescript-document-nodes": "^2.3.5",
    "@graphql-codegen/typescript-operations": "^2.5.5",
    "@graphql-codegen/typescript-resolvers": "^2.2.0",
    "@graphql-codegen/typescript-urql": "^3.7.2",
    "@urql/exchange-retry": "^1.0.0",
    "graphql-tag": "^2.12.6"
  }
}
